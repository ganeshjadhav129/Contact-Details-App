<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.realapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".RealApplication"
        android:allowBackup="true"
        android:icon="@drawable/m_ou"
        android:label="MiaauApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ContactInfo"></activity>
        <activity android:name=".CreateNewContact" />
        <activity android:name=".ContactList" />
        <activity android:name=".Register" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>